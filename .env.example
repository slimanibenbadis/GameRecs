# Database Configuration
POSTGRES_USER=your_db_username
POSTGRES_PASSWORD=your_db_password

# Spring Security
JWT_SECRET=replace_with_secure_random_string_at_least_64_characters
JWT_EXPIRATION=86400000

# IGDB API Configuration
IGDB_CLIENT_ID=your_igdb_client_id_from_twitch_developer_portal
IGDB_CLIENT_SECRET=your_igdb_client_secret_from_twitch_developer_portal

# Spring Profiles
SPRING_PROFILES_ACTIVE=dev

# Backend Configuration
BACKEND_URL=http://localhost:8080
ALLOWED_ORIGINS=http://localhost:4200

# Monitoring
PROMETHEUS_USERNAME=your_prometheus_username
PROMETHEUS_PASSWORD=your_prometheus_password 